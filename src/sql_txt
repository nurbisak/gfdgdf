CREATE TABLE customers
(
    id         SERIAL PRIMARY KEY,
    email      VARCHAR(50),
    first_name VARCHAR(50),
    last_name  VARCHAR(50),
    age        INT,
    weight     INT
);
INSERT INTO customers(email, first_name, last_name, age, weight)
VALUES ('nurbisak@gmail.com', 'Nurbek', 'ISAKOV', 38, 80);

CREATE TABLE car
(
    id INT,
    creat_year        VARCHAR(20),
    model      VARCHAR(20),
    colour    VARCHAR
);
INSERT INTO car(id, creat_year, model, colour)
VALUES(1,20010,'Camry','black');

CREATE TABLE kokboru
(
    team_name         VARCHAR,
    area              VARCHAR,
    general_manager   VARCHAR,
    players       INT
);
INSERT INTO kokboru(team_name, area, general_manager, players)
VALUES('alaman','batken','nurbek',15);



CREATE TABLE devices
(
    entity   VARCHAR,
    brand    VARCHAR,
    price    INT,
    model VARCHAR
);
INSERT INTO devices(entity, brand, price, model)
VALUES('call','sony',45000,'sony_erikson');

CREATE TABLE flowers
(
    type    VARCHAR,
    color   VARCHAR,
    country VARCHAR,
    height  INT,
    count   INT
);
INSERT INTO flowers(type, color, country, height, count)
VALUES ('Rose', 'red', 'Kyrgyzstan', 7, 9);

CREATE TABLE instructors
(
    ful_name VARCHAR,
    region VARCHAR,
    marital_status VARCHAR,
    children INT,
    student INT,
    gender VARCHAR
);
